//problem 01

#include<iostream>
using namespace std;
int main()
{
    string ab;
    cin>>ab;
    bool numeric = true;
    for(int i=0; i<ab.length(); i++)
    {
       //65-90 ; 97-122 ; 48-57
        if(!(ab[i]>=48 && ab[i]<=57) )
        {
            numeric = false;
        }
    }
    if(numeric == true)
        cout << "Numeric constant";
    else
        cout << "Not numeric constant";
    return 0;
}

//problem 02

#include<iostream>
#include<string>
using namespace std;
int main()
{
    string input;
    getline(cin, input);
    char op[100];
    int n = 0;
    for(int i = 0; i < input.length(); i++)
    {
        switch(input[i])
        {
            case '+':
            case '-':
            case '*':
            case '/':
            case '%':
            case '=':
                op[n++] = input[i];
                break;
            default:
                break;
        }
    }

    if(n == 0)
    {
        cout << "There is no operator." << endl;
    }
    else
    {
        for(int i = 0; i < n; i++)
        {
            cout << "operator" << i + 1 << ": " << op[i] << endl;
        }
    }
    return 0;
}


//problem 03
#include<iostream>
using namespace std;

int main()
{
    string a;
    getline(cin, a);

    if(a[0] == '/' && a[1] == '/')
        cout << "Single line comment";
    else if(a[a.length()-1] == '/' && a[a.length()-2] == '*' && a[0] == '/' && a[1] == '*')
        cout << "Multi line comment";
    else
        cout << "Not";

    return 0;
}

//problem 04
#include<iostream>
using namespace std;

int main()
{
    string a;
    cin >> a;

    bool identify = true;
    bool reserveWord = false;

    if(a == "int" || a == "bool" || a == "float" || a == "string" || a == "double" || a == "long" || a == "namespace")
        reserveWord = true;

    if(!((a[0] >= 65 && a[0] <= 90) || (a[0] >= 97 && a[0] <= 122) || (a[0] == 95)))
        identify = false;

    for(int i = 1; i < a.length(); i++)
    {
        if(!((a[i] >= 65 && a[i] <= 90) || (a[i] >= 97 && a[i] <= 122) || (a[i] == 95) || (a[i] >= 48 && a[i] <= 57)))
        {
            identify = false;
            break;
        }
    }

    if(reserveWord)
        cout << "Input is a reserved word";
    else if(identify)
        cout << "Input is identifier.";
    else
        cout << "Input is not identifier.";

    return 0;
}
